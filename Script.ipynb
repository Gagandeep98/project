   "source": [
    "# reverse_number.py\n",
    "\n",
    "user_input=int(input(\"Enter the number to reverse: \"))\n",
    "_rev=0\n",
    "while(user_input>0):\n",
    "  dig=user_input%10\n",
    "  _rev=_rev*10+dig\n",
    "  user_input=user_input//10\n",
    "print(\"The reversed number is :\",_rev)"
   ]
